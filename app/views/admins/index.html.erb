<p id="notice"><%= notice %></p>

<h1>Administradores</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>Correo Electrónico</th>
      <th>Teléfono</th>
      <th>Campus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins.each do |admin| %>
      <tr>
        <td><%= admin.name %></td>
        <td><%= admin.last_name %></td>
        <td><%= admin.email %></td>
        <td><%= admin.telephone %></td>
        <td><%= admin.center.name %></td>
        <td><%= link_to 'Ver', admin %></td>
        <td><%= link_to 'Editar', edit_admin_path(admin) %></td>
        <td><%= link_to 'Eliminar', admin, method: :delete, data: { confirm: '¿Está seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Administrador', new_admin_path %>
